# Include the default Makefile.
include Makefile.linux

# Name of the executable.
PROG	= 86Box64

# Various compile-time options.
STUFF	=
EXTRAS	=
DEBUG	= n
OPTIM	= n
X64	= y
DEV_BRANCH = n

# End of Makefile.linux64.
